@model GetCargoCustomerDto
@{
    ViewData["Title"] = "UserAddressInfo";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<section class="content container-fluid">
	<div class="row">
		<div class="col-md-12">
			<div class="chart-box">
				<div class="bs-example" data-example-id="bordered-table">
					<h3>Kullanıcı Adres ve İletişim Bilgisi</h3>
					<div class="alert alert-info">Kullanıcı Id : @Model.UserCustomerId</div>
					<div class="alert alert-danger">Kullanıcı Adı : @Model.Surname</div>
					<div class="alert alert-warning">Email Adresi : @Model.Email</div>
					<div class="alert alert-warning">Telefon : @Model.Phone</div>
					<div class="alert alert-warning">İlçe-Şehir : @Model.District @Model.City</div>
					<div class="alert alert-warning">Adres : @Model.District @Model.Address</div>
				</div>
			</div>
		</div>
	</div>
</section>

