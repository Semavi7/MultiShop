@model MultiShop.Images.WebUı.DAL.Entities.ImagesDrive
@{
	Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
	<br /><br />
	<h2 class="alert alert-primary">Google Drive Storge Resim Yükleme Sayfası</h2>

	<form method="post" enctype="multipart/form-data">
		<label>Ürün Adı</label>
		<input type="text" class="form-control border" name="Name" />
		<br />
		<label>Dosya Seçin</label>
		<input type="file" class="form-control border" name="Photo" />
		<br />
		<input name="SavedUrl" type="hidden" class="form-control" />
		<input name="SavedFileName" type="hidden" class="form-control" />
		<button class="btn btn-outline-info">Ürün Görseli Kaydet</button>

	</form>
</div>


